<h2>List of Users</h2>
(Number of users: <%= @users.count
%>)
<br></br>
<%= link_to new_admin_user_path do %>
    <span class="glyphicon glyphicon-plus"></span> User
<% end %>

<ul class="users">
  <% @users.each do |u| %>
      <li>
        <%=   u.id %>
        <%=   u.name %>
        <%= link_to  admin_user_path(u) do %>
            <span class="glyphicon glyphicon-eye-open" alt="show"></span>
        <% end %>
      </li>

  <% end %>
</ul>